{
  "name": "win-opacity",
  "displayName": "Windows opacity",
  "description": "Changes opacity of Code windows on Windows using Windows API",
  "version": "1.1.6",
  "publisher": "skacekachna",
  "author": "Jan ZÃ­pek",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.77.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SkaceKamen/vscode-win-opacity.git"
  },
  "extensionKind": [
    "ui"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./src/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Win Opacity Configuration",
      "properties": {
        "winopacity.opacity": {
          "type": "integer",
          "default": 245,
          "description": "Opacity value. Should be between 0 and 255, where 0 is invisible window",
          "minimum": 40,
          "maximum": 255
        }
      }
    }
  },
  "scripts": {
    "rebuild": "npm rebuild --runtime=electron --target=22.3.6 --disturl=https://electronjs.org/headers",
    "package": "vsce package && node build/package.js",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^6.0.40",
    "@types/vscode": "^1.40.0",
    "eslint": "^6.6.0",
    "glob-fs": "^0.1.7"
  },
  "dependencies": {
    "@electron/rebuild": "^3.2.12",
    "bindings": "^1.5.0",
    "ffi-napi": "^4.0.3",
    "install": "^0.13.0",
    "node-addon-api": "^3.1.0",
    "ref-napi": "^3.0.2"
  },
  "license": "MIT"
}
